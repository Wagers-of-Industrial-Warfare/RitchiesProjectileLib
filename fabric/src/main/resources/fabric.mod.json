{
  "schemaVersion": 1,
  "id": "ritchiesprojectilelib",
  "version": "${version}",
  "name": "Ritchie's Projectile Library",
  "description": "A Minecraft modding library for better projectiles.",
  "authors": [
    "rbasamoyai"
  ],
  "contact": {
    "homepage": "https://github.com/Wagers-of-Industrial-Warfare/RitchiesProjectileLib",
    "sources": "https://github.com/Wagers-of-Industrial-Warfare/RitchiesProjectileLib"
  },
  "license": "MIT License",
  "environment": "*",
  "entrypoints": {
    "main": [
      "rbasamoyai.ritchiesprojectilelib.fabric.RitchiesProjectileLibFabric"
    ],
    "client": [
      "rbasamoyai.ritchiesprojectilelib.fabric.RitchiesProjectileLibFabricClient"
    ]
  },
  "mixins": [
    "ritchiesprojectilelib.mixins.json",
    "ritchiesprojectilelib-fabric.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",

    "porting_lib_utility": ">=${porting_lib_version}",
    "porting_lib_networking": ">=${porting_lib_version}"
  }
}